	<PG name="title">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);icon.parentNode.getChildByName('text').attributes.textSize = pageIcon.templateData.title[0].size;if (pageIcon.templateData.title[0].bgImage == undefined && icon.built != 'true'){  icon.nextSibling.removeNode();  icon.built = 'true';}if (pageIcon.templateData.title[0].bgImageAlpha != undefined){  icon.nextSibling.attributes.initObject = '{_alpha:' + pageIcon.templateData.title[0].bgImageAlpha + '}';}titleStr = '<p align="center">' + pageIcon.templateData.title + '</p>'if (pageIcon.templateData.title[0].titleHAlign != undefined){  titleStr = '<p align="' + pageIcon.templateData.title[0].titleHAlign + '">' + pageIcon.templateData.title + '</p>'}if (pageIcon.templateData.title[0].titleTextColour != undefined){  icon.parentNode.getChildByName('text').attributes.textColour = pageIcon.templateData.title[0].titleTextColour.split('0x').join('#');}if (pageIcon.templateData.title[0].narration != undefined){  narration = pageIcon.templateData.title[0].narration;  rootIcon.broadcast('showAudio');}]]></SCR>		<DIS h="516" w="800" name="backgroundImage" x="0" y="0" type="ext" url="{pageIcon.templateData.title[0].bgImage}"><![CDATA[]]></DIS>		<DIS id="txt" name="text" x="20" y="200" w="760" h="475" type="text"><![CDATA[{titleStr}]]></DIS>		<SCR name="centreY"><![CDATA[//topif (pageIcon.templateData.title[0].titleVAlign == '20'){  txt._y = 20;}//middleif (pageIcon.templateData.title[0].titleVAlign == '200' || pageIcon.templateData.title[0].titleVAlign == undefined){  txt._y = 200 - txt._height / 2;}//bottomif (pageIcon.templateData.title[0].titleVAlign == '400'){  txt._y = 514 - txt._height  - 20;}]]></SCR>	</PG>