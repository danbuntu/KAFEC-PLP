	<PG name="buttonQuestion">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);align = pageIcon.templateData.buttonQuestion[0].align;panelWidth = pageIcon.templateData.buttonQuestion[0].panelWidth;if (panelWidth == "Small"){  panelW = 300;  textW = 440;} else if (panelWidth == "Medium"){  panelW = 450;  textW = 290;} else if (panelWidth == "Large"){  panelW = 600;  textW = 150;} else {  panelW = 760;  textW = 0;}if (pageIcon.templateData.buttonQuestion[0].narration != undefined){  narration = pageIcon.templateData.buttonQuestion[0].narration;  rootIcon.broadcast('showAudio');}]]></SCR>		<DIS w="panelW" h="474" tabIndex="-1" id="panel" name="panel" x="align == 'Right' ? 20 : 780 - panelW" y="20" type="ext" url="templatePath + 'common/whitePanel.swf'"><![CDATA[]]></DIS>		<DIS name="instruction" x="align == 'Right' ? 40 + panelW : 20" y="panel._y" w="740 - panelW" h="10" type="text"><![CDATA[{pageIcon.templateData.buttonQuestion[0].instruction}]]></DIS>		<DIS textColour="#333333" id="prpt" name="question" x="panel._x + 20" y="panel._y + 20" w="panelW - 40" h="10" type="text"><![CDATA[{pageIcon.templateData.buttonQuestion[0].prompt}]]></DIS>		<INT name="buttonInteraction" perpetual="0">			<RES rs="btn" swf="templatePath + 'common/button.swf'" name="button" type="button" x="prpt._x" y="prpt._y + prpt._height + 20" label="{pageIcon.templateData.buttonQuestion[0].label}" erase="1" exit="0">				<DIS scrolling="auto" textColour="#333333" name="feedback" x="btn._x" y="btn._y + 40" w="panelW - 40" h="474 - (btn._y + btn._height) - 10" type="text"><![CDATA[{pageIcon.templateData.buttonQuestion[0].feedback}]]></DIS>			</RES>		</INT>	</PG>