<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/nwkc_geolocation/db" VERSION="20110331" COMMENT="XMLDB file for Moodle blocks/nwkc_geolocation test"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="nwkc_geolocation" COMMENT="storage of location information for check-in">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="studentid"/>
        <FIELD NAME="studentid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="timecheckedin"/>
        <FIELD NAME="timecheckedin" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="studentid" NEXT="lat"/>
        <FIELD NAME="lat" TYPE="number" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" ENUM="false" DECIMALS="8" PREVIOUS="timecheckedin" NEXT="lon"/>
        <FIELD NAME="lon" TYPE="number" LENGTH="10" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" ENUM="false" DECIMALS="8" PREVIOUS="lat" NEXT="location"/>
        <FIELD NAME="location" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="lon"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>